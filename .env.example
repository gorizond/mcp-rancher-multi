# Main Rancher server
RANCHER_URL=https://rancher.example.com
RANCHER_TOKEN=your-token-here
RANCHER_USERNAME=admin
RANCHER_PASSWORD=your-password
RANCHER_INSECURE=false
RANCHER_TIMEOUT=30000
RANCHER_RETRIES=3
RANCHER_NAME=default

# Additional servers (up to 10)
RANCHER_SERVER_2_URL=https://rancher2.example.com
RANCHER_SERVER_2_TOKEN=your-token-2-here
RANCHER_SERVER_2_NAME=production-server

RANCHER_SERVER_3_URL=https://rancher3.example.com
RANCHER_SERVER_3_TOKEN=your-token-3-here
RANCHER_SERVER_3_NAME=staging-server

# Default settings
DEFAULT_RANCHER_SERVER=default
LOG_LEVEL=info
CACHE_TIMEOUT=300000
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=30000

# Logging settings (file logging disabled by default)
# Set ENABLE_FILE_LOGGING=true to enable file logging
ENABLE_FILE_LOGGING=false
LOG_DIRECTORY=logs
