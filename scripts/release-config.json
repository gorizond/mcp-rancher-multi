{
  "repository": {
    "owner": "gorizond",
    "name": "mcp-rancher-multi",
    "url": "https://github.com/gorizond/mcp-rancher-multi"
  },
  "release": {
    "template": {
      "includeCommits": true,
      "maxCommits": 20,
      "includeInstallation": true,
      "includeDocker": true,
      "includeBreakingChanges": true,
      "includeNewFeatures": true,
